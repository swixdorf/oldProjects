//---------------------------------------------------------------------------

#include <vcl.h>
#pragma hdrstop

#include "Unit1.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
TForm1 *Form1;
//---------------------------------------------------------------------------
__fastcall TForm1::TForm1(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TForm1::ServerSocket3ClientConnect(TObject *Sender,
      TCustomWinSocket *Socket)
{
	switch (Socket->LocalPort)
	{
		case 18001:
			Form1
		break;
default:
;
}
}
//---------------------------------------------------------------------------
